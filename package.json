{
  "name": "@inops/web-sdk",
  "version": "1.1.1",
  "description": "Inops Web SDK (search + campaignId landing helpers)",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "examples"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs,iife --dts --minify --global-name Inops --out-dir dist --clean",
    "build:watch": "tsup src/index.ts --format esm,cjs,iife --dts --watch --global-name Inops --out-dir dist --clean",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.1.0",
    "typescript": "^5.6.3"
  }
}


